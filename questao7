#include<stdio.h>
#include<stdlib.h>

int main() {
	float peso_prato;
	float por_quilo = 45.00;
	float valor_a_pagar;
	
	printf("\ndigite o peso do prato em quilos\n");
	scanf("%f",&peso_prato);
	
	valor_a_pagar=peso_prato * por_quilo;
	
	printf("\nO valor do prato: %f", valor_a_pagar);
	
	

    
	


	return 0;
}
